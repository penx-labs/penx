{
	"appId": "io.penx.app",
	"appName": "PenX",
	"webDir": "dist",
	"plugins": {
		"SplashScreen": {
			"androidSplashResourceName": "splash",
			"androidScaleType": "CENTER_CROP",
			"backgroundColor": "#ffffffff",
			"showSpinner": false
		},
		"Keyboard": {
			"resize": "none",
			"resizeOnFullScreen": true
		}
	},
	"packageClassList": [
		"DarkModeNative",
		"SafeAreaPlugin",
		"SpeechRecognition",
		"AppPlugin",
		"DevicePlugin",
		"HapticsPlugin",
		"InAppBrowserPlugin",
		"KeyboardPlugin",
		"SplashScreenPlugin",
		"StatusBarPlugin",
		"SocialLoginPlugin",
		"PurchasesPlugin",
		"VoiceRecorder"
	]
}
